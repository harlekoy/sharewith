{
    "name": "harlekoy/sharewith",
    "description": "Document share with handling for Laravel 5.4 and up",
    "keywords": [
        "harlekoy",
        "laravel",
        "sharewith",
        "fileshare",
        "sharing"
    ],
    "homepage": "https://github.com/harlekoy/sharewith",
    "license": "MIT",
    "authors": [
        {
            "name": "Harlequin Doyon",
            "email": "harlequin.doyon@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0",
        "illuminate/database": "~5.4.0|~5.5.0"
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "orchestra/testbench": "~3.3.0|~3.4.2|~3.5.0",
        "phpunit/phpunit" : "^5.7|6.2"
    },
    "autoload": {
        "psr-4": {
            "Harlekoy\\ShareWith\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Harlekoy\\ShareWith\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harlekoy\\ShareWith\\ShareWithServiceProvider"
            ]
        }
    }
}
